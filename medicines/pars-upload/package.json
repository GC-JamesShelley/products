{
  "name": "pars-upload",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "a11y": "yarn authless-server; server_pid=$!; yarn run pa11y-ci; test_exit_code=$?; kill \"$server_pid\"; exit \"$test_exit_code\";",
    "dev": "next dev",
    "authless-server": "NEXT_PUBLIC_DISABLE_AUTH=true yarn dev & wait-on http://localhost:3000",
    "build": "next build",
    "start": "next start",
    "export": "next build && next export",
    "lint": "eslint src/",
    "test-e2e": "yarn authless-server; server_pid=$!; yarn cypress run --env PARS_UPLOAD_URL=$PARS_UPLOAD_URL; test_exit_code=$?; kill \"$server_pid\"; exit \"$test_exit_code\";",
    "test-e2e-headed": "yarn authless-server; server_pid=$!; yarn cypress run --env PARS_UPLOAD_URL=$PARS_UPLOAD_URL --headed --no-exit; test_exit_code=$?; kill \"$server_pid\"; exit \"$test_exit_code\";",
    "test": "yarn jest",
    "test:ci": "yarn jest --coverage --ci"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "core-js": "^3.4.7",
    "govuk-frontend": "^3.6.0",
    "msal": "^1.3.1",
    "next": "^9.4.2",
    "next-compose-plugins": "^2.2.0",
    "next-optimized-images": "^2.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.26.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@testing-library/cypress": "^6.0.0",
    "babel-jest": "^26.0.1",
    "cypress": "^4.6.0",
    "cypress-file-upload": "^3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "imagemin-optipng": "^7.1.0",
    "jest": "^26.0.1",
    "pa11y-ci": "^2.3.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "wait-on": "^5.0.0"
  }
}
