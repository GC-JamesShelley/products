{
  "author": "Red Badger",
  "dependencies": {
    "@types/node-fetch": "^2.5.4",
    "@types/react-gtm-module": "^2.0.0",
    "core-js": "^3.4.7",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "gray-matter": "^4.0.2",
    "marked": "^1.1.1",
    "moment": "^2.24.0",
    "next": "^9.1.4",
    "node-glob": "^1.2.0",
    "raw-loader": "^4.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-ga": "^2.7.0",
    "react-gtm-module": "^2.0.8",
    "react-markdown": "^4.3.1",
    "react-schemaorg": "^1.0.2",
    "schema-dts": "^0.4.5",
    "styled-components": "^4.4.1",
    "styled-normalize": "^8.0.6",
    "universal-cookie": "^4.0.3",
    "whatwg-fetch": "^3.0.0"
  },
  "description": "MHRA Medicine Products Portal",
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@svgr/cli": "^4.3.3",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^25.1.1",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/styled-components": "^4.4.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-styled-components": "^1.10.6",
    "cypress": "^3.8.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "html-loader": "^0.5.5",
    "husky": "^3.0.9",
    "jest": "^25.1.0",
    "lint-staged": "^9.4.3",
    "markdown-loader": "^5.1.0",
    "pa11y-ci": "^2.3.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.1.0",
    "ts-node": "^8.5.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.5",
    "wait-on": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,json,yml,md}": [
      "yarn prettier --write"
    ],
    "*.{ts,tsx}": [
      "yarn lint"
    ],
    "*.tf": "bash -c 'terraform fmt -check'"
  },
  "main": "index.js",
  "name": "mhra-mip-website",
  "private": false,
  "scripts": {
    "a11y": "yarn dev & wait-on http://localhost:3000; server_pid=$!; yarn run pa11y-ci; test_exit_code=$?; kill \"$server_pid\"; exit \"$test_exit_code\";",
    "build": "next build",
    "dev": "next",
    "export": "next export -o dist && yarn sitemap",
    "lint": "yarn tslint",
    "sitemap": "ts-node --project src/tools/tsconfig.json src/tools/sitemap-generator.ts",
    "start": "next start",
    "test-e2e": "yarn dev & wait-on http://localhost:3000; server_pid=$!; yarn run cypress run --env $(cat .env | sed '/^$/d' | tr \"\\n\", \",\" | sed 's/,$/ /g'); test_exit_code=$?; kill \"$server_pid\"; exit \"$test_exit_code\";",
    "test-e2e-interactive": "yarn dev & wait-on http://localhost:3000; server_pid=$!; yarn run cypress run --headed --env $(cat .env | sed '/^$/d' | tr \"\\n\", \",\" | sed 's/,$/ /g'); test_exit_code=$?; kill \"$server_pid\"; exit \"$test_exit_code\";",
    "test": "yarn jest",
    "test:ci": "yarn jest --coverage --ci"
  },
  "version": "1.0.0"
}
