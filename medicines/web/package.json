{
  "author": "Red Badger",
  "dependencies": {
    "@types/node-fetch": "^2.5.7",
    "@types/react-gtm-module": "^2.0.0",
    "core-js": "^3.6.5",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "moment": "^2.27.0",
    "next": "9.2.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.1.2",
    "react-gtm-module": "^2.0.10",
    "react-schemaorg": "^1.2.0",
    "schema-dts": "^0.6.0",
    "styled-components": "^5.1.1",
    "styled-normalize": "^8.0.6",
    "universal-cookie": "^4.0.3",
    "whatwg-fetch": "^3.2.0"
  },
  "description": "MHRA Medicine Products Portal",
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@svgr/cli": "^5.4.0",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.26",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.1",
    "babel-jest": "^26.1.0",
    "babel-plugin-styled-components": "^1.10.6",
    "cypress": "^4.11.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "html-loader": "^1.1.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "markdown-loader": "^5.1.0",
    "pa11y-ci": "^2.3.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0",
    "typescript": "^3.9.7",
    "wait-on": "^5.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,json,yml,md}": [
      "yarn prettier --write"
    ],
    "*.{ts,tsx}": [
      "yarn lint"
    ],
    "*.tf": "bash -c 'terraform fmt -check'"
  },
  "main": "index.js",
  "name": "mhra-mip-website",
  "private": false,
  "scripts": {
    "a11y": "yarn dev & wait-on http://localhost:3000; server_pid=$!; yarn run pa11y-ci; test_exit_code=$?; kill \"$server_pid\"; exit \"$test_exit_code\";",
    "build": "next build",
    "dev": "next",
    "export": "next export -o dist && yarn sitemap",
    "lint": "yarn tslint",
    "sitemap": "ts-node --project src/tools/tsconfig.json src/tools/sitemap-generator.ts",
    "start": "next start",
    "test-e2e": "yarn dev & wait-on http://localhost:3000; server_pid=$!; yarn run cypress run --env $(cat .env | sed '/^$/d' | tr \"\\n\", \",\" | sed 's/,$/ /g'); test_exit_code=$?; kill \"$server_pid\"; exit \"$test_exit_code\";",
    "test-e2e-interactive": "yarn dev & wait-on http://localhost:3000; server_pid=$!; yarn run cypress run --headed --env $(cat .env | sed '/^$/d' | tr \"\\n\", \",\" | sed 's/,$/ /g'); test_exit_code=$?; kill \"$server_pid\"; exit \"$test_exit_code\";",
    "test": "yarn jest",
    "test:ci": "yarn jest --coverage --ci"
  },
  "version": "1.0.0"
}
